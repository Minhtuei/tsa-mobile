{
  "expo": {
    "name": "tsa",
    "slug": "tsa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/TSA_logo",
    "userInterfaceStyle": "light",
    "scheme": "tsa",
    "splash": {
      "image": "./assets/TSA_splash.png",
      "backgroundColor": "#5CC990"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/TSA_adaptive_logo.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./config/google-services.json",
      "package": "com.ttq.tsa"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1IjoicXVhbmNhbzIzMTAiLCJhIjoiY20yNXU4aHVxMHYxdzJxcXNudXh0ZmdiYSJ9.3zLWsysWsrpT-bi5c7hn5w",
          "RNMapboxMapsVersion": "11.0.0"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ]
  }
}
